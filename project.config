	push = group user/Lars Lubkoll (lubkoll)
	read = group user/Lars Lubkoll (lubkoll)
	rebase = group user/Lars Lubkoll (lubkoll)
	removeReviewer = group user/Lars Lubkoll (lubkoll)
	submit = group user/Lars Lubkoll (lubkoll)
[access "refs/heads/*"]
	create = group user/Lars Lubkoll (lubkoll)
	push = +force group user/Lars Lubkoll (lubkoll)
	pushMerge = group user/Lars Lubkoll (lubkoll)
	read = group user/Lars Lubkoll (lubkoll)
[access "refs/tags/*"]
	push = group user/Lars Lubkoll (lubkoll)
